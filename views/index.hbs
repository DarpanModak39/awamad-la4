<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <nav>
      <a href="/">Home</a>
      <a href="/add">Add Photos</a>
    </nav>
    <div>
      <h1>Welcome To Photograpy Website</h1>
    </div>
    <table>
      <tr>
        <td>Category</td>
        <td>Image</td>
        <td>Author</td>
      </tr>

      {{#each data}}
        <tr>
          <td>{{category}}</td>
          <td><img src="./images/{{photo}}" alt="image" /></td>
          <td>{{name}}</td>

        </tr>
      {{/each}}

    </table>
  </body>
</html>